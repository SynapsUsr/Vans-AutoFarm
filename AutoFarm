--Vans AutoFarm

-- Instances:

local AutoFarm = Instance.new("ScreenGui")
local Main = Instance.new("Frame")
local Enable = Instance.new("TextButton")
local Disable = Instance.new("TextButton")
local Shut = Instance.new("TextButton")

--Properties:

AutoFarm.Name = "AutoFarm"
AutoFarm.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
AutoFarm.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Main.Name = "Main"
Main.Parent = AutoFarm
Main.BackgroundColor3 = Color3.fromRGB(66, 66, 66)
Main.BackgroundTransparency = 0.700
Main.Position = UDim2.new(0.147344798, 0, 0.233128846, 0)
Main.Size = UDim2.new(0, 943, 0, 434)

Enable.Name = "Enable"
Enable.Parent = Main
Enable.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Enable.Position = UDim2.new(0.0371155888, 0, 0.44470048, 0)
Enable.Size = UDim2.new(0, 202, 0, 122)
Enable.Font = Enum.Font.SourceSans
Enable.Text = "Enable"
Enable.TextColor3 = Color3.fromRGB(0, 0, 0)
Enable.TextScaled = true
Enable.TextSize = 14.000
Enable.TextWrapped = true

Disable.Name = "Disable"
Disable.Parent = Main
Disable.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Disable.Position = UDim2.new(0.317073166, 0, 0.44470048, 0)
Disable.Size = UDim2.new(0, 202, 0, 122)
Disable.Font = Enum.Font.SourceSans
Disable.Text = "Disable"
Disable.TextColor3 = Color3.fromRGB(0, 0, 0)
Disable.TextScaled = true
Disable.TextSize = 14.000
Disable.TextWrapped = true

Shut.Name = "Shut"
Shut.Parent = AutoFarm
Shut.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Shut.Position = UDim2.new(0.797307432, 0, 0.233128831, 0)
Shut.Size = UDim2.new(0, 74, 0, 50)
Shut.Font = Enum.Font.SourceSans
Shut.Text = "Close"
Shut.TextColor3 = Color3.fromRGB(0, 0, 0)
Shut.TextSize = 14.000

-- Scripts:

local function EXYLZRB_fake_script() -- Main.Handler 
	local script = Instance.new('LocalScript', Main)

	local enabled = false
	
	local enab = script.Parent.Enable
	
	local disab = script.Parent.Disable
	
	while true do
		wait(10)
		if enabled == true then
			game:GetService("ReplicatedStorage").TGSCoins_ClaimCoin:FireServer("1")
		end
	end
	
	enab.MouseButton1Click:Connect(function()
		enabled = true
	end)
	
	disab.MouseButton1Click:Connect(function()
		enabled = false
	end)
end
coroutine.wrap(EXYLZRB_fake_script)()
local function UOZAWF_fake_script() -- Shut.Close 
	local script = Instance.new('LocalScript', Shut)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(UOZAWF_fake_script)()
